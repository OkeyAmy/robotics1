# Copy this file to .env and fill in values. Do not commit .env.

# Server
PORT=3000

# PostgreSQL (defaults match docker-compose; override for remote DB)
# PGUSER=admin
# PGHOST=localhost
# PGDATABASE=robotics_v1
# PGPASSWORD=your_password
# PGPORT=5432

# Redis (optional; app defaults to localhost:6379)
# REDIS_URL=redis://localhost:6379

# Gemini AI (optional; for ai_server.js)
# GEMINI_API_KEY=
